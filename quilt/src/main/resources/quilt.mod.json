{
  "schema_version": 1,
  "mixin": [
    "endlessmusic.mixins.json",
    "endlessmusic-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_title}",
      "description": "${mod_description}",
      "contributors": {
        "Me!": "Owner"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/endlessmusic/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.endlessmusic.quilt.EndlessMusicImpl"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}