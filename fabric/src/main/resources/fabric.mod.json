{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${modVersion}",
	"name": "${modName}",
	"description": "${modDescription}",
	"authors": [
		"${modAuthor}"
	],
	"contact": {
		"homepage": "${modHomepage}",
		"sources": "${modSources}",
		"issues": "${modIssues}"
	},
	"license": "${modLicense}",
	"icon": "${modId}.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.odinokland.constantmusic.fabric.ConstantMusicFabric"
		],
		"preLaunch": [
			"com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
		],
		"modmenu": [
			"com.odinokland.constantmusic.fabric.ConstantMusicModMenuIntegration"
		]
	},
	"mixins": [
		"${modId}.common.mixins.json",
		"${modId}.fabric.mixins.json"
	],
	"accessWidener": "${modId}.accesswidener",
	"depends": {
		"minecraft": ">=${minMinecraftVersion}-",
		"fabricloader": "*",
		"fabric": "*",
		"java": ">=${javaVersion}"
	},
	"suggests": {
		"modmenu": ">=${modMenuVersion}"
	}
}